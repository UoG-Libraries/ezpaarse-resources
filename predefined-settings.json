    "country": "France",
    "headers": { "Double-Click-Removal": "false"}
  },
  "00-fr-inist-DCMixed10": {
    "fullName": "INIST-CNRS DoubleClikMixed-10",
    "country": "France",
    "headers": { 
      "Output-Fields": "+datetime",
      "Double-Click-MIXED":"10"  
    }
